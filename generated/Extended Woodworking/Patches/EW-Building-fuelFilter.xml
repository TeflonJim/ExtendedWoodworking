<Patch>
    <!-- Core -->
    <Operation Class="PatchOperationAdd">
        <success>Always</success>
        <xpath>Defs/ThingDef[category="Building" or contains(thingClass, "Building")]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
        <value>
            <li>WoodLog</li>
            <li>WoodLog_Birch</li>
            <li>WoodLog_Cecropia</li>
            <li>WoodLog_Cypress</li>
            <li>WoodLog_Drago</li>
            <li>WoodLog_Maple</li>
            <li>WoodLog_Oak</li>
            <li>WoodLog_Palm</li>
            <li>WoodLog_Pine</li>
            <li>WoodLog_Poplar</li>
            <li>WoodLog_Teak</li>
            <li>WoodLog_Willow</li>
        </value>
    </Operation>
    <!-- VGP Xtra Trees and Flowers -->
    <Operation Class="PatchOperationFindMod">
        <success>Always</success>
        <mods>
            <li>VGP Xtra Trees and Flowers</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[category="Building" or contains(thingClass, "Building")]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
            <value>
                <li>WoodLog_Acacia</li>
                <li>WoodLog_Camellia</li>
                <li>WoodLog_CherryBlossom</li>
                <li>WoodLog_JapaneseMaple</li>
                <li>WoodLog_RedMaple</li>
            </value>
        </match>
    </Operation>
    <!-- Nature's Pretty Sweet -->
    <Operation Class="PatchOperationFindMod">
        <success>Always</success>
        <mods>
            <li>Nature's Pretty Sweet</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[category="Building" or contains(thingClass, "Building")]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
            <value>
                <li>RedWoodLog</li>
            </value>
        </match>
    </Operation>
</Patch>