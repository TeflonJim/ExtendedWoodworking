      <li>GloomyFurniture</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="RGK_Flower"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="RGK_Flower" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="RGK_Flower"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="RGK_Flower"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="RGK_Flower" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="RGK_Flower"]</xpath>
            <value>
              <costStuffCount>15</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="RGK_Flower"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="RGK_Flower"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
