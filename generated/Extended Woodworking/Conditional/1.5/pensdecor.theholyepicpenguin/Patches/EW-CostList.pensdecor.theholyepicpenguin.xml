        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="PensTreeOne" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="PensTreeOne"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="PensTreeOne"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="PensTreeOne" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="PensTreeOne" and not(costStuffCount)]</xpath>
            <value>
              <costStuffCount>10</costStuffCount>
            </value>
          </match>
