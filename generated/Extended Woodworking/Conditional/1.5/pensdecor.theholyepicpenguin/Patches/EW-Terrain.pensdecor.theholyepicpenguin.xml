    <operations>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="WoodLog_Acacia"]</xpath>
        <match Class="PatchOperationSequence">
          <operations>
            <li Class="PatchOperationAdd">
              <xpath>/Defs</xpath>
              <value>
                <TerrainDef ParentName="FloorBase">
                  <defName>PenWoodFloor_Acacia</defName>
                  <label>homely acacia wood floor</label>
                  <renderPrecedence>250</renderPrecedence>
                  <description>A colorful acacia plank floor. Pleasing to look at and quick to build, but it is also very flammable.</description>
                  <texturePath>Terrain/Surfaces/penwoodfloor</texturePath>
                  <burnedDef>BurnedWoodPlankFloor</burnedDef>
                  <color>(219, 184, 144)</color>
                  <statBases>
                    <Beauty>1</Beauty>
                    <WorkToBuild>240</WorkToBuild>
                    <Flammability>0.22</Flammability>
                  </statBases>
                  <constructEffect>ConstructWood</constructEffect>
                  <costList>
                    <WoodLog_Acacia>3</WoodLog_Acacia>
                  </costList>
                  <designatorDropdown>EW_Designator_PenWoodFloor</designatorDropdown>
                </TerrainDef>
              </value>
            </li>
          </operations>
        </match>
