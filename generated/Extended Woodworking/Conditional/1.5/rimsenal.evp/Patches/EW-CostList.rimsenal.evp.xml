<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="Gun_Blowgun"]/costList/WoodLog</xpath>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName="Gun_Blowgun"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="Gun_Blowgun" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="Gun_Blowgun"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="Gun_Blowgun"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="Gun_Blowgun" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="Gun_Blowgun" and not(costStuffCount)]</xpath>
            <value>
              <costStuffCount>30</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="Gun_Blowgun"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef[defName="Gun_Blowgun"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
