        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="WoodLog_Acacia"]</xpath>
        <match Class="PatchOperationSequence">
          <operations>
            <li Class="PatchOperationAdd">
              <xpath>/Defs</xpath>
              <value>
                <TerrainDef ParentName="FloorBase">
                  <defName>CCot_RusticPlankFloor_Acacia</defName>
                  <label>rustic plank floor</label>
                  <renderPrecedence>250</renderPrecedence>
                  <description>Rustic plank flooring for that warm, homey feeling. While it is quick to lay, it is also very flammable.</description>
                  <texturePath>Terrain/Surfaces/RusticPlanks</texturePath>
                  <burnedDef>BurnedWoodPlankFloor</burnedDef>
                  <color>(219, 184, 144)</color>
                  <statBases>
                    <WorkToBuild>85</WorkToBuild>
                    <Flammability>0.22</Flammability>
                    <Beauty>1</Beauty>
                  </statBases>
                  <constructEffect>ConstructWood</constructEffect>
                  <costList>
                    <WoodLog_Acacia>6</WoodLog_Acacia>
                  </costList>
                  <designatorDropdown>EW_Designator_CCot_RusticPlankFloor</designatorDropdown>
                </TerrainDef>
              </value>
            </li>
          </operations>
        </match>
