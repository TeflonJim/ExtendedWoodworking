        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="WoodLog_Acacia"]</xpath>
        <match Class="PatchOperationSequence">
          <operations>
            <li Class="PatchOperationAdd">
              <xpath>/Defs</xpath>
              <value>
                <TerrainDef ParentName="FloorBase">
                  <defName>Boards_Totemic_Acacia</defName>
                  <label>totemic boards</label>
                  <description>Fine acacia boards decorated in a totemic style.</description>
                  <texturePath>Terrain/Surfaces/TotemicBoards</texturePath>
                  <renderPrecedence>250</renderPrecedence>
                  <canGenerateDefaultDesignator>false</canGenerateDefaultDesignator>
                  <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
                  <dominantStyleCategory>Totemic</dominantStyleCategory>
                  <color>(219, 184, 144)</color>
                  <isPaintable>true</isPaintable>
                  <statBases>
                    <WorkToBuild>5000</WorkToBuild>
                    <Beauty>3</Beauty>
                    <StyleDominance>1</StyleDominance>
                    <Flammability>0.22</Flammability>
                  </statBases>
                  <tags>
                    <li>FineFloor</li>
                  </tags>
                  <costList>
                    <WoodLog_Acacia>20</WoodLog_Acacia>
                  </costList>
                  <designatorDropdown>EW_Designator_Boards_Totemic</designatorDropdown>
                </TerrainDef>
              </value>
            </li>
          </operations>
        </match>
