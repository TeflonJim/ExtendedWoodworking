  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="RoughBarricade"]/costList/WoodLog</xpath>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName="RoughBarricade"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RoughBarricade" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RoughBarricade"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RoughBarricade"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RoughBarricade" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RoughBarricade" and not(costStuffCount)]</xpath>
            <value>
              <costStuffCount>2</costStuffCount>
            </value>
          </match>
