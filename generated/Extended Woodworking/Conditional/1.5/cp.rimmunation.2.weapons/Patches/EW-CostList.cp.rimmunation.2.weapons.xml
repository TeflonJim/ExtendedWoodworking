<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe" and not(costStuffCount)]</xpath>
          <value>
            <costStuffCount>30</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Fire_Axe"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe" and not(costStuffCount)]</xpath>
          <value>
            <costStuffCount>35</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_Felling_Axe"]/constructEffect</xpath>
        </match>
      </li>
      <li Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat"]/costList/WoodLog</xpath>
      </li>
      <li Class="PatchOperationConditional">
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat" and (stuffCategories)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat"]/stuffCategories</xpath>
          <value>
            <li>Woody</li>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat"]</xpath>
          <value>
            <stuffCategories>
              <li>Woody</li>
            </stuffCategories>
          </value>
        </nomatch>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat" and not(costStuffCount)]</xpath>
        <match Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat" and not(costStuffCount)]</xpath>
          <value>
            <costStuffCount>55</costStuffCount>
          </value>
        </match>
      </li>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat"]/constructEffect</xpath>
        <match Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="RNMeleeWeapon_BrooklynSmasher_Bat"]/constructEffect</xpath>
        </match>
