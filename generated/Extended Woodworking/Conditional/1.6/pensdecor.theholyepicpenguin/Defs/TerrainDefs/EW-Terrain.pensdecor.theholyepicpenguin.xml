<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <TerrainDef ParentName="FloorBase">
    <defName>PenWoodFloor_Birch</defName>
    <label>homely birch wood floor</label>
    <renderPrecedence>250</renderPrecedence>
    <description>A colorful birch plank floor. Pleasing to look at and quick to build, but it is also very flammable.</description>
    <texturePath>Terrain/Surfaces/penwoodfloor</texturePath>
    <burnedDef>BurnedWoodPlankFloor</burnedDef>
    <color>(235,228,216)</color>
    <statBases>
      <Beauty>1</Beauty>
      <WorkToBuild>240</WorkToBuild>
      <Flammability>0.22</Flammability>
    </statBases>
    <constructEffect>ConstructWood</constructEffect>
    <costList>
      <WoodLog_Birch>3</WoodLog_Birch>
    </costList>
    <designatorDropdown>EW_Designator_PenWoodFloor</designatorDropdown>
  </TerrainDef>
