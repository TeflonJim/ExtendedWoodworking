<Defs>
  <RecipeDef>
    <defName>PaintWoodLog_Black</defName>
    <label>paint wood black</label>
    <description>Paints wood black</description>
    <jobString>Painting.</jobString>
    <workAmount>300</workAmount>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>WoodTypes</li>
          </categories>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <products>
      <WoodLog_Black>25</WoodLog_Black>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>WoodTypes</li>
      </categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>WoodTypes</li>
      </categories>
      <disallowedThingDefs>
        <li>WoodLog_Black</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <recipeUsers>
      <li>TableWoodworking</li>
    </recipeUsers>
  </RecipeDef>
