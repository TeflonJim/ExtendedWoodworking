<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <operations>
      <li Class="PatchOperationConditional">
        <success>Always</success>
        <xpath>/Defs/ThingDef[defName="WoodLog_Acacia"]</xpath>
        <match Class="PatchOperationSequence">
          <operations>
            <li Class="PatchOperationAdd">
              <xpath>/Defs</xpath>
              <value>
                <TerrainDef ParentName="FloorBase">
                  <defName>WoodPlankFloor_Acacia</defName>
                  <label>acacia wood floor</label>
                  <renderPrecedence>250</renderPrecedence>
                  <description>Acacia plank flooring for that warm, homey feeling. While it is quick to lay, it is also very flammable.</description>
                  <texturePath>Terrain/Surfaces/WoodFloor</texturePath>
                  <burnedDef>BurnedWoodPlankFloor</burnedDef>
                  <color>(219, 184, 144)</color>
                  <pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainFadeRoughSoftLight</pollutionShaderType>
                  <pollutionOverlayTexturePath>Terrain/Surfaces/PollutionFloorSmooth</pollutionOverlayTexturePath>
                  <isPaintable>true</isPaintable>
                  <statBases>
                    <WorkToBuild>85</WorkToBuild>
                    <Flammability>0.22</Flammability>
                  </statBases>
                  <constructEffect>ConstructWood</constructEffect>
                  <designationHotKey>Misc9</designationHotKey>
                  <uiOrder>2000</uiOrder>
                  <costList>
                    <WoodLog_Acacia>3</WoodLog_Acacia>
                  </costList>
                  <designatorDropdown>EW_Designator_WoodPlankFloor</designatorDropdown>
                </TerrainDef>
              </value>
            </li>
            <li Class="PatchOperationAdd">
              <xpath>/Defs</xpath>
              <value>
                <TerrainDef>
                  <defName>Bridge_Acacia</defName>
                  <label>bridge</label>
                  <description>A flat acacia surface on supportive beams which can be built over water. You can build light structures on bridges, but be careful - bridges are fragile. If a bridge falls, buildings on top of it fall as well.</description>
                  <texturePath>Terrain/Surfaces/WoodFloor</texturePath>
                  <color>(219, 184, 144)</color>
                  <uiIconPath>Terrain/Surfaces/Bridge_MenuIcon</uiIconPath>
                  <layerable>true</layerable>
                  <bridge>true</bridge>
                  <affordances>
                    <li>Light</li>
                  </affordances>
                  <designationCategory>Structure</designationCategory>
                  <fertility>0</fertility>
                  <constructEffect>ConstructWood</constructEffect>
                  <terrainAffordanceNeeded>Bridgeable</terrainAffordanceNeeded>
                  <edgeType>Hard</edgeType>
                  <renderPrecedence>400</renderPrecedence>
                  <autoRebuildable>true</autoRebuildable>
                  <statBases>
                    <WorkToBuild>1500</WorkToBuild>
                    <Flammability>0.8</Flammability>
                  </statBases>
                  <tags>
                    <li>Floor</li>
                  </tags>
                  <designationHotKey>Misc4</designationHotKey>
                  <destroyOnBombDamageThreshold>35</destroyOnBombDamageThreshold>
                  <destroyBuildingsOnDestroyed>true</destroyBuildingsOnDestroyed>
                  <destroyEffect>Bridge_Collapse</destroyEffect>
                  <destroyEffectWater>Bridge_CollapseWater</destroyEffectWater>
                  <costList>
                    <WoodLog_Acacia>12</WoodLog_Acacia>
                  </costList>
                  <designatorDropdown>EW_Designator_Bridge</designatorDropdown>
                </TerrainDef>
              </value>
            </li>
          </operations>
        </match>
      </li>
