<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <success>Always</success>
    <mods>
      <li>Camping Stuff</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="DeployableTent"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="DeployableTent" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="DeployableTent"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="DeployableTent"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="DeployableTent" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="DeployableTent"]</xpath>
            <value>
              <costStuffCount>25</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="DeployableTent"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="DeployableTent"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
