<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <success>Always</success>
        <mods>
            <li>%MOD%</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[defName="Plant_Tree%NAME%"]/plant</xpath>
                    <value>
                        <harvestedThingDef>WoodLog_%NAME%</harvestedThingDef>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>