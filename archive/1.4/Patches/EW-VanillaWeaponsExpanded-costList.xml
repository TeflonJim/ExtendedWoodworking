<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <success>Always</success>
    <mods>
      <li>Vanilla Weapons Expanded</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]</xpath>
            <value>
              <costStuffCount>100</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Crossbow"]/constructEffect</xpath>
          </match>
        </li>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]</xpath>
            <value>
              <costStuffCount>80</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Musket"]/constructEffect</xpath>
          </match>
        </li>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]</xpath>
            <value>
              <costStuffCount>30</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="VWE_Gun_Flintlock"]/constructEffect</xpath>
          </match>
        </li>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Long" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Long" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]</xpath>
            <value>
              <costStuffCount>65</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="VWE_Bow_Long"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>