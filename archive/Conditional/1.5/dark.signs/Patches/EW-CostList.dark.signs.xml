    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName="SignWoodenMulti"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="SignWoodenMulti" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="SignWoodenMulti"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="SignWoodenMulti"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="SignWoodenMulti" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="SignWoodenMulti" and not(costStuffCount)]</xpath>
            <value>
              <costStuffCount>20</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="SignWoodenMulti"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef[defName="SignWoodenMulti"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
