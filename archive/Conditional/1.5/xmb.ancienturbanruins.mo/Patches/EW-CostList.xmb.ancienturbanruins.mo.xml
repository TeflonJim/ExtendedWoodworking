<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[defName="AM_ADAR"]/costList/WoodLog</xpath>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[defName="AM_ADAR"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="AM_ADAR" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="AM_ADAR"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="AM_ADAR"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="AM_ADAR" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="AM_ADAR" and not(costStuffCount)]</xpath>
            <value>
              <costStuffCount>30</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="AM_ADAR"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>Defs/ThingDef[defName="AM_ADAR"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
