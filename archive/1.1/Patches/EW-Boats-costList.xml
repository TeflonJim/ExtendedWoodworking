<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <success>Always</success>
    <mods>
      <li>Boats</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]/costList/WoodLog</xpath>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint" and (stuffCategories)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]/stuffCategories</xpath>
            <value>
              <li>Woody</li>
            </value>
          </match>
          <nomatch Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]</xpath>
            <value>
              <stuffCategories>
                <li>Woody</li>
              </stuffCategories>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint" and not(costStuffCount)]</xpath>
          <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]</xpath>
            <value>
              <costStuffCount>225</costStuffCount>
            </value>
          </match>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]/constructEffect</xpath>
          <match Class="PatchOperationRemove">
            <xpath>/Defs/ThingDef[defName="SPBoats_RowboatBlueprint"]/constructEffect</xpath>
          </match>
        </li>
      </operations>
    </match>
  </Operation>
